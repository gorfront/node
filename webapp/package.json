{
  "name": "@ideanick/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache 'src/**/*.scss'",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "preview": "vite preview",
    "types": "pnpm tsc --noEmit --project ./tsconfig.json && pnpm tsc --noEmit --project ./tsconfig.node.json",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "@ideanick/backend": "workspace:*",
    "@tanstack/react-query": "^5.89.0",
    "@trpc/client": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.17.20",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "include-media": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-infinite-scroller": "^1.2.6",
    "reset-css": "^5.0.2",
    "sass": "^1.92.1",
    "superjson": "^2.2.2",
    "use-debounce": "^10.0.6",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-infinite-scroller": "^1.2.5",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-prisma": "^5.0.0",
    "stylelint": "^16.24.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "stylelint-scss": "^6.12.1",
    "vite": "^7.1.2",
    "vite-plugin-svgr": "^4.5.0"
  }
}
